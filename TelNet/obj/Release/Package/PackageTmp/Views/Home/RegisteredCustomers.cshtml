
@{
    Layout = "~/Views/Home/_LayoutCRM.cshtml";
}



<!DOCTYPE html>

<html lang="en">

<body class="g-sidenav-show" style="background-color: #FFFF">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/dataTables.bootstrap5.min.js"></script>
    <!--   Core JS Files   -->
    <script src="~/Content/assets/js/core/popper.min.js"></script>
    <script src="~/Content/assets/js/core/bootstrap.min.js"></script>
    <script src="~/Content/assets/js/plugins/perfect-scrollbar.min.js"></script>
    <script src="~/Content/assets/js/plugins/smooth-scrollbar.min.js"></script>
    <script src="~/Content/assets/js/plugins/chartjs.min.js"></script>
    <main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg ">
        <div class="container mb-4 container-round" style="padding-top:100px">
            <h4>Registered Customers</h4>
            <table id="myTable" class="table table-hover table-responsive ">
                <thead>
                    <tr style="text-align:center">
                        <th>Application ID</th>
                        <th>Account Number</th>
                        <th>Account Password</th>
                        <th>Telephone Number</th>
                        <th>Service Plan</th>
                    </tr>
                </thead>
                <tbody>

                    @foreach (var customerAccounts in Model)
                    {
                        <tr style="text-align:center">
                            <td>@customerAccounts.applicationId</td>
                            <td>@customerAccounts.accountNumber</td>
                            <td>@customerAccounts.accountPassword</td>
                            <td>@customerAccounts.telephoneNumber</td>
                            <td>@customerAccounts.serviceAvailed</td>
                        </tr>

                    }
                </tbody>
            </table>

        </div>




    </main>

    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/dataTables.bootstrap5.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#myTable').DataTable();
        });
    </script>
</body>

</html>